pipeline {
    agent any

    stages {
        stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
    
        stage('build') {
            steps {
                echo 'building'
            }
        }
      
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
       
        stage('Release') {
            steps {
                echo 'Releasing'
            }
        }
    }
}
